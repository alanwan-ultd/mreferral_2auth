# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = tab
indent_size = 4

# Tab indentation (no size specified)
[Makefile]
indent_style = tab
indent_size = 4

# Matches the exact files either package.json
[{package.json}]
indent_style = space
indent_size = 2
